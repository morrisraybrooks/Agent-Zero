{
  "name": "PromptArchitect",
  "version": "1.0.0",
  "description": "Specialized prompt engineering sub-agent for Agent Zero. Crafts, analyzes, optimizes, debugs, reverse-engineers, and audits prompts and system prompt architectures for any LLM.",
  "type": "sub_agent",
  "parent_agent": "agent_zero",

  "model_config": {
    "chat_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-20250514",
      "temperature": 0.4,
      "max_tokens": 8192,
      "top_p": 0.9,
      "context_window": 200000
    },
    "utility_model": {
      "provider": "openai",
      "model": "gpt-4o-mini",
      "temperature": 0.2,
      "max_tokens": 4096,
      "context_window": 128000
    },
    "embedding_model": {
      "provider": "openai",
      "model": "text-embedding-3-large",
      "dimensions": 3072
    }
  },

  "prompts": {
    "role": "agent.system.main.role.md",
    "communication": "agent.system.main.communication.md",
    "environment": "agent.system.main.environment.md",
    "context": "_context.md"
  },

  "memory": {
    "enabled": true,
    "backend": "chromadb",
    "collection": "prompt_architect_memory",
    "auto_memorize": true,
    "memory_categories": [
      "prompt_patterns",
      "successful_prompts",
      "failed_prompts",
      "system_prompt_templates",
      "jailbreak_patterns",
      "defense_patterns",
      "model_specific_behaviors",
      "user_preferences"
    ],
    "retrieval": {
      "top_k": 10,
      "similarity_threshold": 0.72,
      "rerank": true
    }
  },

  "tools": {
    "enabled": [
      "code_execution",
      "file_read",
      "file_write",
      "web_
